from pyzbar.pyzbar import decode
from PIL import Image


def read_qr_code():
#performs decoding of qr_code.png image and printing of each codes details
    for i in range(30):
        qr_code_decode = decode(Image.open('./students/student_{0}.png'.format(i)))
        student_data_split = qr_code_decode[0]
        print(student_data_split[0])

